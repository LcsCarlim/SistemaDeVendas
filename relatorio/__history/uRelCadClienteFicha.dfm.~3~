  object QryCliente: TZQuery
    Connection = dtmPrincipal.ConexaoDB
    Active = True
    SQL.Strings = (
      'select  clienteId,'
      '        nome,'
      '        cep,'
      '        bairro,'
      '        cidade,'
      '        endereco,'
      '        estado,'
      '        email,'
      '        telefone,'
      '        dataNascimento'
      '  from  clientes'
      'order by nome')
    Params = <>
    Left = 688
    Top = 328
